{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "rootDir": "../",
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "allowImportingTsExtensions": true
  },
"include": ["./**/*.ts", "../shared/**/*.ts"],
"exclude": ["../node_modules"]
}